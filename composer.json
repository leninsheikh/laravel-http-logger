{
    "name": "leninsheikh/laravel-http-logger",
    "type": "library",
    "version": "0.0.1",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Leninsheikh\\LaravelHttpLogger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lenin Sheikh",
            "email": "justtlenin@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": ">=8",
        "illuminate/http": ">=8",
        "illuminate/database": ">=8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leninsheikh\\LaravelHttpLogger\\Providers\\LaravelHttpLoggerServiceProvider",
                "Leninsheikh\\LaravelHttpLogger\\Providers\\EventServiceProvider"
            ]
        }
    }
}
